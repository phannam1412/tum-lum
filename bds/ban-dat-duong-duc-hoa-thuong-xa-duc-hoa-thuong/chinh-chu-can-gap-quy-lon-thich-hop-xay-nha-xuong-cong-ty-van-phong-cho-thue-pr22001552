var LoginModule = undefined;
$(function () {
if (getCookie('userinfo') !== undefined && getCookie('userinfo') != '') {
$.get('/HandlerWeb/UserHandler.ashx?type=getuserstatus',
function (data) {
if (data !== undefined && data != null) {
var html = '<div><div class="header-middle-righ-link"><a href="javascript:;" onclick="LogOut()" rel="nofollow">Thoát</a></div><div class="header-middle-righ-icon"><img src="https://file4.batdongsan.com.vn/images/ab/logout.png" id="ico_logout" style="padding-top: 2px !important"></div></div><div><div class="header-middle-righ-link max155 user_name"><img src="https://file4.batdongsan.com.vn/images/ab/user.png" style="display: block; float: left; margin-top: 7px; margin-right: 3px;"><a style="white-space: nowrap;" href="' + data.cplnk + '" rel="nofollow">' + data.name + '</a></div><div class=\"header-middle-righ-icon\"  id=\"chat-quick-inbox-icon\" style=\"height: 30px; margin-top: 0px; margin-right: 2px; position: relative; width: 25px; background: url(https://file4.batdongsan.com.vn/images/ab/chat.png) no-repeat 0 7px;\"><img src=\"https://file4.batdongsan.com.vn/images/ab/user.png\" style=\"display: none;\"><span class=\"mnu-chat-icon-unread\"></span></div></div>';
if (data.notify > 0) {
html += '<div class="header-middle-righ-icon nqv" onclick="ShowQuickViewNotify(this);" style="height: 30px;position: relative;width: 25px;background: url(https://file4.batdongsan.com.vn/images/ab/thong-bao.png) no-repeat 0 7px;"><span class="mnu-notify-icon-unread nqv">' + data.notify + '</span></div>';
}
$('#divUserStt').html(html);
}
else {
LoadLoginModule();
}
$('#divUserStt').show();
}, 'json');
} else {
$('#divUserStt').show();
LoadLoginModule();
}
});
function LogOut() { try { BDSChat.LogOutSingR(false); } catch (ex) { }; window.location.href = '/Logout.aspx'; }
function LoadLoginModule() {
if (window.location.href.indexOf('trang-dang-nhap') < 0 && window.location.href.indexOf('dang-ky-nguoi-dung') < 0 && window.location.href.indexOf('lay-lai-mat-khau') < 0 && window.location.href.indexOf('trang-ca-nhan') < 0 && window.location.href.indexOf('thay-doi-mat-khau') < 0) {
if (LoginModule !== undefined) {
_loadLoginModuleSuccess();
} else {
//$('body').append('<link href="/css/login.css?v=' + (new Date().getTime()) + '" rel="stylesheet" />');
//var ua = window.navigator.userAgent;
//var msie = ua.indexOf("MSIE ");
//if (msie > 0)
//    $('body').append('<link href="/css/login-ie.css?v=' + (new Date().getTime()) + '" rel="stylesheet" />');
writeScript('/Modules/UserProfile/Scripts/account-login.js?v=2');
}
}
}
window._loadLoginModuleSuccess = function () {
$('#divUserStt').find('a[href="/trang-dang-nhap"]').click(function () {
LoginModule.showLoginForm();
});
$('#divUserStt').find('a[href="/trang-dang-nhap"]').attr('href', '#');
$('#divUserStt').find('a[href="/dang-ky-nguoi-dung"]').click(function () {
LoginModule.showRegistForm();
});
$('#divUserStt').find('a[href="/dang-ky-nguoi-dung"]').attr('href', '#');
}
</script>
