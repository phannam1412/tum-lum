<script type="text/javascript">
var urlCrypted = $('.product-list').attr('pagekey');
$(document).ready(function () {
$(".background-pager-right-controls").find('div').css("width", "30px !important");
});
function sortchange() {
setCookie('psortfilter', $('#ddlSortReult').val() + '$' + $('ul.filter > li.active').attr('rel') + '$' + urlCrypted, 1);
}
function ChangeTypeListProduct(type) {
setCookie('psortfilter', $('#ddlSortReult').val() + '$' + type + '$' + urlCrypted, 1);
}
function linkPostAdNewClick(data) {
setCookie('POST_ADNEW_LOCATION', data, 1);
return true;
}
function displayProjectInfo() {
$(".project-container").css("display", "block");
$("#LeftMainContent__productSearchResult_ltrTagsLocation").hide();
$(".search-productItem").hide();
$("#LeftMainContent__productSearchResult_pnlNotFound").hide();
//$(".Repeat").hide();
$("ul.header-tab li").removeClass("active");
$("#liProjectInfo").addClass("active");
}
$(function () {
var cookie = getCookie('psortfilter');
if (cookie != undefined && cookie != null && cookie.length > 0) {
var arr = cookie.split('$');
if (arr.length == 3) {
if (urlCrypted != arr[2]) {
setCookie('psortfilter', '', 0);
}
}
}
$('.p-title > h3 > a').each(function () {
$(this).html($(this).html().replace(/đặc khu/ig, ''));
});
$('.p-main > .p-content > .p-main-text').each(function () {
$(this).html($(this).html().replace(/đặc khu/ig, ''));
});
});
</script>
