<script type="text/javascript">
var defaultKeyword = "Nhập từ khóa để tìm theo cụm từ";
var hdbTopSearch = $('#home-top-search').AdvanceHiddenDropbox({
id: 'home-top-search-otions',
hddValue: 'cboTypeSearch'
});
function Redirect() {
var strInput = $("#txt1").val().toLowerCase();
if (strInput.length == 0 || $.trim(strInput) == '' || $.trim(strInput) == defaultKeyword) {
$("#txt1").focus();
//return false;
}
else {
strInput = strInput.replace(/'/ig, '');
var strOrigin = strInput;
strInput = UnicodeToKoDauUrl(strInput);
var url = "";
var typeSearch = $("#cboTypeSearch").val();
if (typeSearch == "1")
url = "/tags/ban/" + strInput;
else if (typeSearch == "2")
url = "/tags/cho-thue/" + strInput;
else if (typeSearch == "3")
url = "/tag/" + strInput;
else if (typeSearch == "4")
url = "/dien-dan-hoi-dap-bat-dong-san?k=" + strInput;
else if (typeSearch == "5")
url = "/phan-muc-cac-du-an-bds?k=" + strOrigin;
else if (typeSearch == "6")
url = "/doanh-nghiep-cac-linh-vuc?k=" + strOrigin;
else if (typeSearch == "7")
url = "/nha-moi-gioi?k=" + strOrigin;
window.location.href = url;
}
}
$(document).ready(function () {
//$("#txt1").val(defaultKeyword);
$("#txt1").addClass("txtKeyword");
$("#txt1").click(function () {
$("#txt1").focus();
});
$("#txt1").focus(function () {
if ($("#txt1").val().length == 0 || $.trim($("#txt1").val()) == '' || $.trim($("#txt1").val()) == defaultKeyword)
$("#txt1").val("");
});
$("#txt1").blur(function () {
var strInput = $("#txt1").val();
if (strInput.length == 0 || $.trim(strInput) == '' || $.trim(strInput) == defaultKeyword)
$("#txt1").val(defaultKeyword);
});
$('#txt1').keypress(function (e) {
if (e.which == 13) {
Redirect();
return false;
}
});
if ($.browser.opera || ($.browser.msie && $.browser.version.split('.')[0] < 7)) {
$('.home-top-search .select-box').css('opacity', '1');
}
else {
// select element styling
$('.home-top-search .select-box').each(function () {
var title = $(this).attr('title');
if ($('option:selected', this).val() != '') title = $('option:selected', this).text();
$(this).css({ 'z-index': 10, 'opacity': 0, '-khtml-appearance': 'none' });
$(this).after('<span class="select-text"><span class="select-text-content" style="width: 102px; height: 26px; line-height: 26px; color: grey;">' + title + '</span></span>');
$(this).change(function () {
var val = $('option:selected', this).text();
$(this).next().children(":first").text(val);
});
});
}
if (document.referrer.indexOf("www.google.com") > 0 && window.location.href != '/') {
$('#gglbody').html('<span id="bigfont" style="font-size:14px;">Chào mừng bạn đến với Batdongsan.com.vn từ Google! Bạn có biết</span><span> Batdongsan.com.vn là kênh thông tin số 1 về bất động sản và các lĩnh vực liên quan với <span class="redfont"><strong>240.000</strong> lượt truy cập</span> và <span class="redfont"><strong>15.000</strong> tin mới</span> mỗi ngày? Hãy sử dụng <a href="https://batdongsan.com.vn" title="Batdongsan.com.vn">Batdongsan.com.vn</a> để thỏa mãn tốt nhất mọi nhu cầu của bạn!</span>');
$('#gglbody').show();
}
});
</script>
