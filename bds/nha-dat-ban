<script>
(function ($) {
$.support.placeholder = ('placeholder' in document.createElement('input'));
})(jQuery);
//fix for IE7 and IE8
$(function () {
if (!$.support.placeholder) {
$("[placeholder]").css("color", "#555");
$("[placeholder]").focus(function () {
if ($(this).val() == $(this).attr("placeholder")) $(this).val("");
}).blur(function () {
if ($(this).val() == "") $(this).val($(this).attr("placeholder"));
}).blur();
$("[placeholder]").parents("form").submit(function () {
$(this).find('[placeholder]').each(function () {
if ($(this).val() == $(this).attr("placeholder")) {
$(this).val("");
}
});
});
}
});
</script>
<script type="text/javascript">
$(document).ready(function () {
$(".newsletter-register").click(function (e) {
e.preventDefault();
var email = $('#email').val().trim();
$(".newsletter-register").attr("href", "/Modules/Newsletter/Frontend/Register.aspx?email=" + email);
return true;
});
$(".newsletter-register").fancybox({
'width': 660,
'height': 616,
'maxWidth': 660,
'minHeight': 360,
'autoScale': false,
'transitionIn': 'none',
'transitionOut': 'none',
'type': 'iframe',
'scrolling': 'no',
'iframe': { 'scrolling': 'no' },
'autoSize': true,
padding: 0,
wrapCSS: 'fancybox-newsletter'
});
});
function ChangeNewsletterStyle() {
$(".newsletter-form-small").css("border", "none").addClass("box-left");
$(".newsletter-form-small .mb5").html("<div class=\"box-title\">Cập nhật thông tin thị trường</div>").removeClass("mb5");
$(".newsletter-content").addClass("search-form");
$(".newsletter-content #email").css("border", "none").css("width", "130px").css("padding","0").css("border-radius", "0");
}
</script>
